@{
    ViewData["Title"] = "Securely Convert Assets with TechnoApp  Confirmation";
}

@section Header {
    <meta name="description" content="Experience reliable assets conversion. Confirm your transactions securely with TechnoApp  , ensuring data integrity and peace of mind." />
}
<section class="section-upload bg2">
    <div class="container">
        <div class="wallet-userdiv33">
            <span class="market-title-h2 headcss33">
                <span>Convert </span>
            </span>
        </div>
        <div class="row">
            <div class="col-md-8 col-12">
                <div class="css77-buyc-outer">

                    <div id="deposit-tabdiv" class="css77-buyc-div mt-4 mb-4">
                        <div class="css77buy-box">                   
                                <form id="msform">
                                    <!---------------------------------------------->
                                    <!-- progressbar -->
                                    <!---------------------------------------------->
                                    <div class="shadow_TechnoSavvy">
                                        <ol class="progressdiv">
                                            <li class="step  is-complete" data-step="1">                                              
                                                    Enter Details                                             
                                            </li>
                                            <li class="step is-active" data-step="2">                                              
                                                    Confirmation                                               
                                            </li>
                                            <!-- <li class="step" data-step="3"> <a href="uploaded-document.html">
                                              Upload Documents</a>
                                            </li> -->

                                        </ol>
                                        <!---------------------------------------------->

                                    </div>
                                    <!---------------------------------------------->
                                    <!-- fieldsets -->
                                    <div class="row">
                                     
                                        <div class="form-card">
                                            <div class="css77buy-box">
                                                <!--------------------confirmation on process------------------------------------>
                                                <div class="confpenddiv">
                                                    <div class="check-depdiv55">
                                                        <div class="css44timediv ">
                                                            <svg class="timer">
                                                                <circle class="progress-frame" cx="100" cy="100" r="50"></circle>
                                                                <circle class="progress-time" cx="100" cy="100" r="50"
                                                                        style="stroke-dasharray: 314.159; stroke-dashoffset: 36.2491;"></circle>
                                                            </svg>
                                                            <div class="time">
                                                                <p><span class="seconds">30</span></p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <h5 class="orange">Convert Pending</h5>

                                                </div>
                                                <!--------------------confirmation not complete so show this div------------------------------------>
                                                <div class="confpenddiv">
                                                    <div class="modegred34">
                                                        <div class="vericond432">
                                                            <i class="fa fa-exclamation d-orange"
                                                               aria-hidden="true"></i>
                                                        </div>
                                                        <h5 class="orange">Convert Pending!</h5>
                                                        <div class="specdivid mb-1">
                                                            <span>Request ID </span>
                                                            <span><a href="#">45bghjdf2145 </a></span>
                                                        </div>

                                                        <div class="csstime55-text cssfun44-div text-center">
                                                            Your convert request for <span class="orange">1 BTC </span>has been submitted. <br>Check your <a asp-controller="wallet" asp-action="FundingWallet"> Funding Wallet</a> after some time.
                                                        </div>
                                                    </div>
                                                    <!------------>
                                                </div>
                                                <!------------>
                                                <!--------------------why Deposit success show this div------------------------------------>
                                                <div class="confpenddiv">
                                                    <div class="modegred34">
                                                        <div class="vericond432"> <i class="fa fa-check d-green" aria-hidden="true"></i></div>

                                                        <h5 class="basegreen">Convert Confirmation</h5>
                                                        <div class="specdivid mb-1">
                                                            <span>TxID </span>
                                                            <span><a href="#">45bghjdf2145 </a></span>
                                                        </div>

                                                        <div class="text-div44">
                                                            <div class="csstime55-text cssfun44-div mt-3">
                                                                Your convert request for <span class="orange">1 BTC </span> has been confirmed. <br> Check your <a asp-controller="wallet" asp-action="FundingWallet"> Funding Wallet </a>.


                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-------------------------------------------------------------------->
                                                    <div class="dflexcenter2">
                                                        <a class="btn withsumm-btn  mt-3 " type="button"
                                                           asp-controller="wallet" asp-action="index">
                                                            Acknowledge
                                                        </a>
                                                    </div>
                                                    <!-------------------------------------------------------------------->
                                                </div>


                                            </div>
                                        </div>
                                        <!-----========================================---->
                                    </div>
                                </form>
                                   </div>
                    </div>

                </div>


            </div>

            <div class="col-md-4 col-12">
                @await Html.PartialAsync("partial/_RightFAQ")

            </div>

        </div>

        <!----------------------------------------------->

    </div>
</section>
@section Scripts{
     
    @if (ConfigEx.ShouldMini)
    {
        <script src="~/minjs/select.min.js"></script>
        <script src="~/minjs/images-select.min.js"></script>
    }
    else
    {
        <script src="~/js/select.js"></script>
        <script src="~/js/images-select.js"></script> 
    }
   
    <script type="text/javascript">
        $(document).ready(function () {
            LoadCurrIfNot();
            formateAll();
            $(".my-select").chosen({ width: "100%" });
        });
    </script>
}