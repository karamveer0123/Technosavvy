@model vmBase
@{
    ViewData["Title"] = "WithdrawCrypto";
}

<section class="section-upload bg2">
    <div class="container">
        <div class="wallet-userdiv33">
            <span class="market-title-h2 headcss33">
                <span>Withdraw </span>
            </span>

        </div>
        <div class="row">
            <div class="col-md-8 col-12">
                <div class="css77-buyc-outer">
                    <div id="deposit-tabdiv" class="css77-buyc-div mt-4 mb-4">
                        <div class="css77buy-box">             
                                <form id="msform" autocomplete="off">
                                    <div class="shadow_TechnoSavvy">
                                        <ol class="progressdiv">
                                            <li class="step  is-complete" data-step="1">                                                
                                                    Enter Details                                                
                                            </li>
                                            <li class="step is-complete" data-step="2">                                            
                                                    Summary                                               
                                            </li>
                                            <li class="step is-active" data-step="3">                                               
                                                    2F Verification                                               
                                            </li>
                                            <li class="step" data-step="4">                                               
                                                    Confirmation                                             
                                            </li>
                                        </ol>
                                    </div>
                                    <div class="authMultFac d-orange">
                                        <i class="fa fa-lock" aria-hidden="true"></i>
                                        <i class="fa fa-check" aria-hidden="true"></i>
                                    </div>
                                    <div class="row">  
                                        <div class="form-card">
                                            <div class="verificationwithdraw-form">
                                                <div class="csstime55-dep mt-2 ">
                                                    <h5>2F Verification</h5>
                                                    <div class="css77-orderdt px-5">
                                                        <div class="csssum-title55">Request ID</div>
                                                        <div class="csssum-detail55">
                                                            <span> kgjfhkjgf25464kfhmjgklm</span>
                                                        </div>
                                                    </div>
                                                    <div class="css77-orderdt px-5">
                                                        <div class="csssum-title55">Amount</div>
                                                        <div class="csssum-detail55">
                                                            <span> 10 BNB </span>
                                                        </div>
                                                    </div>
                                                    <div class="css77-orderdt px-5">
                                                        <div class="csssum-title55">Address</div>
                                                        <div class="csssum-detail55">
                                                            <span> fghgfh143524B </span>
                                                        </div>
                                                    </div>
                                                    <div class="css77-orderdt px-5">
                                                        <div class="csssum-title55">Network </div>
                                                        <div class="csssum-detail55">
                                                            <span> ERC 20 </span>
                                                        </div>
                                                    </div>

                                                </div>
                                                <div class="css33withformdiv">

                                                    <div class="form-group mt-4 clear">
                                                        <label class="form-label" for="pwd">E-mail verification code</label>
                                                        <input type="text" class="form-control" placeholder=" " name="pswd">                                                      
                                                        <div class="enter56"> Enter the 6-digit code received by ashu****.com</div>
                                                    <span class="resend112 gettimer">OTP valid for <span id="countdowntimer">15</span>
                                                        minutes.  </span>
                                                    </div>

                                                    <div class="form-group mt-4 clear">
                                                        <label class="form-label" for="pwd">Google verification code</label>
                                                        <input type="text" class="form-control" placeholder=" " name="pswd">
                                                        <div class="enter56"> Enter the 6-digit code from Google Authenticator.</div>
                                                      
                                                    </div>


                                                </div>
                                            </div>
                                            <span class="dflexdiv">
                                                <a asp-controller="withdraw" asp-action="summary" type="button" name="previous" class="d-dark action-button">Previous</a>
                                                <a asp-controller="withdraw" asp-action="confirmation" type="button" name="next" class=" d-orange action-button float-right">Continue</a>
                                            </span>
                                        </div>
                                    </div>
                                </form>                   

                        </div>
                    </div>

                </div>


            </div>

            <div class="col-md-4 col-12">
                @await Html.PartialAsync("partial/_RightFAQ")

            </div>

        </div>

        <!----------------------------------------------->

    </div>
</section>
@section Scripts{
    @if (ConfigEx.ShouldMini)
    {
        <script src="~/minjs/select.min.js"></script>
        <script src="~/minjs/images-select.min.js"></script>
    }
    else
    {
        <script src="~/js/select.js"></script>
        <script src="~/js/images-select.js"></script>
    }


    <script type="text/javascript">

        $(".my-select").chosen({ width: "100%" });
    </script>
       }