    @section Scripts{
    <script>
        function moveText(e) {
            var x = $(e).html();
            $('#showroad-text').html();
            $('#showroad-text').html(x);
        }
        $('.timeline-content p').hide();
        $('.timeline-content h3').on('click', function () {
            var txt = $(this).parent().children('p');
            txt.slideToggle();
        })
    </script>
    }