@section Scripts{
    @if (ConfigEx.ShouldMini)
    {
        <script src="~/minjs/vertical-tab.min.js"> </script>        
    }
    else
    {
        <script src="~/js/vertical-tab.js"> </script>        
    }

    <script>
        function openNav() {
            $("#mySidepanel").css({ "width": "400px" });
        }

        function closeNav() {
            $("#mySidepanel").css({ "width": "0px" });
        }
        function tablinks() {
            $("#mySidepanel").css({ "width": "0px" });
        }
        $('.tablinks').on('click', function () {
            $("#mySidepanel").css({ "width": "0px" });
        })
    </script>
   
    <script>
        bindaccordionSearch('txtSearchTopic', 'accordionFlushTopic');
        bindaccordionSearch('txtSearchTopicDesktop', 'academyTopicDesktop');

        $(function () {

            if (ta.length > 0)
                $('#' + ta + ' button').trigger('click');
            if (to.length > 0)
                openCity(event, to);
        });
    </script>
}
