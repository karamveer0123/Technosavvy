                    <div>
                        <div class="inner-wrapper-sticky accordion accordion-flush tab-col actabdiv434 academy-tab-col" id="accordionFlushTopic">
                            <form class="positionrelative">
                            <input id="txtSearchTopicDesktop" type="search" class="market-search" autocomplete="off" placeholder="Search" name="search">
                                <span class="searchbtn22"><i class="fa fa-search" aria-hidden="true"></i></span>
                            </form>
                        <div id="academyTopicDesktop"  class="rigthscroll543">
                               
                                <!-- Trade tab -->
