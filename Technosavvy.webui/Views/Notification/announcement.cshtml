                            </form>
                            <div class="rigthscroll543" id="academyTopicDesktop">

                                <!-- Trade tab -->
                                <div class="accordion-item">
                                    <h2 class="accordion-header" id="marketpairtab">
                                        <button class="tablinks active" onclick="openCity(event, 'marketpair')" type="button">
