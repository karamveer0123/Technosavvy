@{
    ViewData["Title"] = " Streamlined and Secure Fiat Deposit on TechnoApp  ";
}

@section Header {
    <meta name="description" content="Easily fund your digital assets with TechnoApp  Fiat Deposit. Experience a streamlined and secure process for depositing fiat currencies into your account." />

}

<section class="section-upload bg2">
    <div class="container">
        <div class="wallet-userdiv33">
            <span class="market-title-h2 headcss33">              
                <span>Deposit </span>
            </span>         
        </div>
        <div class="row">
            <div class="col-md-8 col-12">
                <div class="css77-buyc-outer">
                    <div id="deposit-tabdiv" class="css77-buyc-div mt-4 mb-4">
                        <div class="css77buy-box">
                            <div class="acoun-box authenti-box">
                                <form id="msform" autocomplete="off">                                
                                    <div class="shadow_TechnoSavvy">
                                        <ol class="progressdiv">
                                            <li class="step  is-complete" data-step="1">
                                                 Choose Asset                                               
                                            </li>
                                            <li class="step is-active" data-step="2">
                                               Enter <br>Bank Detail                                               
                                            </li>
                                            <li class="step" data-step="3">                                              
                                                    Check <br>Information
                                            </li>
                                            <li class="step" data-step="4">                                             
                                                    Completed                                              
                                            </li>
                                        </ol>                                    
                                    </div>

                                    <div class="row">                                       
                                        <div class="form-card">
                                            <!-------------------------------------------->
                                            <div class="css77buy-box">
                                                <!-------===========================---->
                                                <div class="assetseprate">
                                                    <div class="form-group mt-3">
                                                        <label class="form-label">
                                                            Select the asset you would like to
                                                            deposit
                                                        </label>
                                                        <div class="form-select3">
                                                            <select class="my-select " aria-label="Deposit">
                                                                <option>INR - ₹</option>
                                                                <optgroup label="Token">
                                                                    <option data-img-src="/images/coin/coin/coin/BNB.png">BNB</option>
                                                                    <option data-img-src="/images/coin/coin/BTC.png">BTC</option>
                                                                    <option data-img-src="/images/coin/coin/BUSD.png">BUSD</option>
                                                                    <option data-img-src="/images/coin/coin/CHZ.png">CHZ</option>
                                                                    <option data-img-src="/images/coin/coin/DAI.png">DAI</option>
                                                                    <option data-img-src="/images/coin/coin/ETH.png">ETH</option>
                                                                    <option data-img-src="/images/coin/coin/LINK.png">LINK</option>
                                                                    <option data-img-src="/images/coin/coin/LUNC.png">LUNC</option>
                                                                    <option data-img-src="/images/coin/coin/MATIC.png">MATIC</option>
                                                                    <option data-img-src="/images/coin/coin/TechnoSavvy.png">TechnoSavvy</option>
                                                                    <option data-img-src="/images/coin/coin/USDC.png">USDC</option>
                                                                    <option data-img-src="/images/coin/coin/USDT.png">USDT</option>
                                                                    <option data-img-src="/images/coin/coin/wBTC.png">wBTC</option>
                                                                </optgroup>
                                                                <optgroup label="Currency">
                                                                    <option>INR - ₹</option>
                                                                    <option>CHF - FR</option>
                                                                    <option>BRL - R$</option>
                                                                    <option>USD - $</option>
                                                                    <option>AUD - $</option>
                                                                    <option>CAD - $</option>
                                                                    <option>CLP - $</option>
                                                                    <option>CNY - ¥</option>
                                                                    <option>CZK - KČ</option>
                                                                    <option>DKK - KR</option>
                                                                    <option>EUR - €</option>
                                                                    <option>GBP - £</option>
                                                                    <option>MYR - RM</option>
                                                                    <option>HKD - $</option>
                                                                    <option>HUF - FT</option>
                                                                    <option>IDR - RP</option>
                                                                    <option>ILS - ₪</option>
                                                                    <option>INR - ₹</option>
                                                                    <option>JPY - ¥</option>
                                                                    <option>KRW - ₩</option>
                                                                    <option>MXN - $</option>
                                                                </optgroup>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!-------===========================---->
                                                <!-------===========================---->
                                                <div class="amountseprate">
                                                    <div class="form-group mt-3">
                                                        <label class="form-label"> Amount</label>
                                                        <input type="text" class="form-control" placeholder="Enter Amount"
                                                             >
                                                    </div>

                                                    <!---------------------------->
                                                    <div class="form-group mt-3">
                                                        <label class="form-label">Deposit Method</label>
                                                        <div class="methoutdiv">

                                                            <div class="methdiv432">
                                                                <div class="meticon"><img src="/images/bank.png"> </div>
                                                                <div class="metname">
                                                                    <span>Bank Transfer</span>
                                                                    <span class="metstate33">
                                                                        NEFT/RTGS/IMPS | Processing Time:Upto 6 hours during 9:00 AM on
                                                                        bank working days
                                                                    </span>
                                                                </div>
                                                                <div class="metfee">Fees:10.00 </div>
                                                            </div>
                                                            <!---------------------->

                                                            <div class="methdiv432">
                                                                <div class="meticon"><img src="/images/UPI.png"> </div>
                                                                <div class="metname">
                                                                    <span>UPIs</span>
                                                                    <span class="metstate33">
                                                                        Processing Time: Upto 6 hours during 9:00 AM to 9:00 PM on bank
                                                                        working days
                                                                    </span>
                                                                </div>
                                                                <div class="metfee">Fees: 15.00 INR</div>
                                                            </div>
                                                            <!---------------------->


                                                        </div>
                                                    </div>

                                                    <a asp-controller="deposit" asp-action="choosebankaccount" type="button" name="next" class=" action-button  gent-btn">Continue</a>
                                                    <!---------------------------->
                                                </div> 
                                            </div>                                        
                                        </div>                                      

                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-12">
                @await Html.PartialAsync("partial/_RightFAQ")

            </div>

        </div>

        <!----------------------------------------------->

    </div>
</section>
@section Scripts{
    
    @if (ConfigEx.ShouldMini)
    {
        <script src="~/minjs/select.min.js"></script>
        <script src="~/minjs/images-select.min.js"></script>
        <script src="~/js/mutiselect.js"></script>
    }
    else
    {
        <script src="~/js/select.js"></script>
        <script src="~/js/images-select.js"></script>
        <script src="~/js/mutiselect.js"></script>
    }


    <script type="text/javascript">

        $(".my-select").chosen({ width: "100%" });
    </script>
       }