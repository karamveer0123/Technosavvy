@{
    ViewData["Title"] = "KycUploadSefie";
}

<section class="section-login bg2">
    <div class="container">
        <div class="login-box" id="login-tab">
            <div class="row">
                <div class="col-md-6 col-xs-12">
                    <div class="person-ver">
                        <h1 class="heading ">Personal Identity Verifcation</h1>
                        <h3> 3 Facial Verifcation </h3>
                        <div class="css7-kycver">
                            <div class="css7-kycdoc-div">
                                <div class="title text-center"> Upload Selfie</div>
                                <div class="css7-kycdoctype">
                                    <div class="row">
                                        <div class="page-selfie-box">
                                            <span class="pic-kyc"> <i class="fa fa-camera" aria-hidden="true"></i> </span>
                                            <span>
                                                <input type="file" name="file-1[]" id="file-1" class="inputfile inputfile-1" data-multiple-caption="{count} files selected" multiple="">
                                                <label for="file-1" class="upload-btn">
                                                    <i class="fa fa-upload" aria-hidden="true"></i>
                                                    <span>Upload Selfie</span>
                                                </label>
                                            </span>
                                        </div>
                                    </div>
                                    <div class="row px-2">
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox" value=""  checked>
                                            <label class="form-check-label" >
                                                By clicking submission. I agree to accept TechnoApp<br>
                                                Terms&Conditions and Privacy Statements.
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="css7-kyc-btn">
                            <button class="btn gent-btn mt-3" type="button" data-bs-toggle="modal" data-bs-target="#selfiModal">Upload & Continue & Submit </button>

                        </div>
                    </div>
                </div>

                <div class="col-md-6 col-xs-12">
                    <div class="login-img"> <img src="/images/login-img.png"> </div>
                </div>
            </div>
        </div>
    </div>
</section>




