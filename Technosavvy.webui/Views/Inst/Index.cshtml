<!-- Labs Contact us -->
<div id="labs-contact">
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <div class="p-0">
                    <h2 class="labs-inner-heading">
                        Get in Touch!

                    </h2>
                    <p>
                        Contact us today to schedule a consultation with our team. Together, let's unlock the
                        potential of cryptocurrency market and drive
                        your institution's growth and success in the digital era.
                    </p>
                </div>
            </div>
            <div class="col-md-6">
                <div class="labs-form">
                    <form>
                        <div class="row g-2">
                            <div class="col-md-6">
                                <div class="mb-0">
                                    <label for="txtName">Name <sup class="text-danger">*</sup></label>
                                    <input type="text" class="form-control" id="txtName" placeholder="Enter Name">
                                    <small class="invalid-feedback">* This feld is mandatory! </small>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="mb-0">
                                    <label for="txtEmail">Email <sup class="text-danger">*</sup></label>
                                    <input type="email" class="form-control " id="txtEmail"
                                           placeholder="Enter E-mail">
                                    <small class="invalid-feedback">* This feld is mandatory! </small>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="mb-0">
                                    <label for="dropEnquiryType">Enquiry Type <sup class="text-danger">*</sup></label>
                                    <div class="form-control foconselect2 ">
                                        <select class="form-select my-select" id="dropEnquiryType"
                                                aria-label="Default select example">
                                            <option selected>Select Enquiry Type</option>
                                            <option value="token-listing">Token Listing</option>
                                            <option value="entity-services">Entity Services</option>
                                            <option value="maintain-liquidity">Maintain Liquidity</option>
                                        </select>
                                    </div>
                                    <small class="invalid-feedback">* This feld is mandatory! </small>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="mb-2">
                                    <label for="txtMessage">Message</label>
                                    <textarea class="form-control" id="txtMessage" rows="3"
                                              placeholder="Additional Message........"></textarea>
                                </div>
                            </div>
                            <div class="col-12 text-right">
                                <button>Submit</button>
                            </div>
                        </div>

                        <div class="mb-3">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>